{
  "name": "vs-frontend-integration",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "schema-utils": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "yargs": "^15.3.1"
  },
  "scripts": {
    "build": "npm-run-all build:frontend build:integrate",
    "build:dev": "npm-run-all build:frontend:dev build:integrate",
    "build:ssr": "npm-run-all build:frontend:ssr \"build:integrate --src-path=../frontend/dist/ssr/client\"",
    "build:ssr:dev": "npm-run-all build:frontend:ssr:dev \"build:integrate --src-path=../frontend/dist/ssr/client\"",
    "build:integrate": "webpack --config webpack.config.js",
    "build:frontend": "yarn --cwd ../frontend/ library",
    "build:frontend:dev": "yarn --cwd ../frontend/ library:dev",
    "build:frontend:ssr": "yarn --cwd ../frontend/ ssr:build",
    "build:frontend:ssr:dev": "yarn --cwd ../frontend/ ssr:build:dev",
    "install:frontend": "yarn --cwd ../frontend/",
    "preinstall": "yarn install:frontend"
  }
}
