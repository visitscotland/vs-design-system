{
  "name": "vs-dotcom-frontend",
  "version": "0.1.0",
  "description": "visitscotland.com Frontend",
  "author": "lukenofurther",
  "main": "dist/library/manifest.json",
  "files": [
    "dist/library"
  ],
  "homepage": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "system": "node ./build/system server --remote-profile hippo",
    "system:build": "node ./build/system build",
    "library": "cross-env NODE_ENV=production webpack --config ./build/library.webpack.conf.js",
    "library:dev": "cross-env NODE_ENV=development webpack --config ./build/library.webpack.conf.js",
    "ssr": "npm-run-all ssr:build ssr:serve",
    "ssr:build": "npm-run-all ssr:build:client ssr:build:server",
    "ssr:build:dev": "cross-env NODE_ENV=development yarn ssr:build",
    "ssr:serve": "nodemon ./ssr/server/index.js",
    "ssr:serve:dev": "cross-env NODE_ENV=development yarn ssr:serve",
    "ssr:build:client": "webpack --config ./build/ssr.client.webpack.conf.js",
    "ssr:build:server": "webpack --config ./build/ssr.server.webpack.conf.js",
    "lint": "eslint",
    "lint:all": "yarn lint \"src/**/*.{js,vue}\"",
    "lint:fix": "yarn lint \"src/**/*.{js,vue}\" --fix",
    "lint:watch": "esw \"src/**/*.{js,vue}\" --watch --ext .js,.vue --watch-delay 0 --fix",
    "lint:style": "stylelint",
    "lint:style:all": "lint:style \"src/**/*.{vue,scss}\"",
    "test": "jest --config ./test/unit/jest.conf.js",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:debug:chrome": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand --config ./test/unit/jest.conf.js",
    "test:e2e": "cypress open",
    "theo": "theo ./src/tokens/tokens.yml --transform web --format map.scss,scss,raw.json,json --dest ./src/assets/tokens",
    "theo:watch": "onchange \"./src/tokens/*.yml\" -- npm run _theo",
    "presystem": "yarn theo",
    "presystem:build": "yarn theo",
    "pressr:build": "yarn theo",
    "pretest": "yarn theo",
    "pretest:watch": "yarn theo",
    "pretest:coverage": "yarn theo",
    "pretest:debug:chrome": "yarn theo",
    "pretest:e2e": "yarn theo"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vue.js",
    "visit scotland",
    "vue",
    "design system",
    "design",
    "system"
  ],
